@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">AmazonFoodStore</h1>
</div>
<div class="container" style="margin-top: 10px;">
    <div class="row">
        <form name="myForm" onsubmit="return validateForm()" method="post">
            <label class="control-label col-md-4">Filter</label>
            <div class="col-md-8">
                <input type="text" name="fn" class="form-control" />
            </div>
            <div class="col-md-8">
                <button type="button" class="btn btn-default" id="filterBtn">Search</button>
                <button onclick="x(this)"></button>
            </div>
        </form>
    </div>
    <table id="AmazonFood" class="myTable">
        <thead>
            <tr>
                <th> LastDayQtySold</th>
                <th> LastDayProfit</th>
                <th> TotalProducts</th>
            </tr>
        </thead>
    </table>
</div>
